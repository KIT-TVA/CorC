<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cbcmodel="http://www.example.org/cbcmodel">
  <cbcmodel:JavaVariables>
    <variables name="a" confidentiality="low" type="Email" modifier="capsule"/>
    <variables name="b" confidentiality="low" type="Email" modifier="capsule"/>
    <variables name="c" confidentiality="low" type="Email" modifier="capsule"/>
  </cbcmodel:JavaVariables>
  <cbcmodel:CbCFormula name="capsuleReference">
    <statement name="statement" context="low">
      <refinement xsi:type="cbcmodel:CompositionStatement" name="compositionStatement" context="none">
        <postCondition name=""/>
        <preCondition name="pre"/>
        <firstStatement name="statement1" context="low">
          <refinement name="b = a;" context="none" typeableResult="true" tyleableText="b = a: Security levels are equal.">
            <postCondition name="intermediateCond"/>
            <preCondition name="pre"/>
          </refinement>
          <postCondition name="intermediateCond">
            <confToVarsMap key="a"/>
          </postCondition>
          <preCondition name="pre"/>
        </firstStatement>
        <secondStatement name="statement2" context="low">
          <refinement name="c = a;" context="none" typeableResult="false" tyleableText="c = a: Value a has the type modifier CAPSULE and was already used before.">
            <postCondition name="post"/>
            <preCondition name="intermediateCond"/>
          </refinement>
          <postCondition name="post"/>
          <preCondition name="intermediateCond">
            <confToVarsMap key="a"/>
          </preCondition>
        </secondStatement>
        <intermediateCondition name="intermediateCond">
          <confToVarsMap key="a"/>
        </intermediateCondition>
      </refinement>
      <postCondition name="post"/>
      <preCondition name="pre"/>
    </statement>
    <security level="low"/>
    <security level="low"/>
    <security level="low"/>
  </cbcmodel:CbCFormula>
</xmi:XMI>
