                        <preCondition name="modifiable(newWithdraw,result,tmp);&#xD;&#xA;(a.withdraw = old_a.withdraw &amp; newWithdraw >= a.withdraw &amp; (tmp = TRUE -> old_a.balance - x = a.balance) &amp; (tmp = FALSE -> old_a.balance = a.balance)) &amp; (tmp = FALSE)"/>
                      </refinement>
                      <postCondition name="modifiable(newWithdraw,result,tmp);&#xD;&#xA;(result = FALSE ->&#xD;&#xA; (a.withdraw = old_a.withdraw &amp;  a.balance = old_a.balance))&#xD;&#xA; &amp; (result = TRUE ->&#xD;&#xA; ((a.withdraw >= old_a.withdraw ) &amp;  a.balance = old_a.balance - x))"/>
                      <preCondition name="modifiable(newWithdraw,result,tmp);&#xD;&#xA;(a.withdraw = old_a.withdraw &amp; newWithdraw >= a.withdraw &amp; (tmp = TRUE -> old_a.balance - x = a.balance) &amp; (tmp = FALSE -> old_a.balance = a.balance)) &amp; (tmp = FALSE)"/>
                    </commands>
