                            <preCondition name="modifiable(newWithdraw);(newWithdraw = a.withdraw - x &amp; x&lt;0) &amp; (newWithdraw &lt; DAILY_LIMIT)"/>
                          </refinement>
                          <postCondition name="modifiable(newWithdraw,result);&#xD;&#xA;a.withdraw = old_a.withdraw &amp; newWithdraw >= a.withdraw"/>
                          <preCondition name="modifiable(newWithdraw);(newWithdraw = a.withdraw - x &amp; x&lt;0) &amp; (newWithdraw &lt; DAILY_LIMIT)"/>
